{
  "build": {
    "content": [
      {
        "src": "azure",
        "files": [
            "**/*.yml",
            "**/*.md"
        ],
        "exclude": [
            "**/includes/**",
            "**/design/**"
        ],
        "dest": "modules"
      },
      {
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "**/design/**",
          "_themes/**",
          "_themes.pdf/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ],
        "files": [
          "**/*.md",
          "**/*.yml"
        ]
      }
    ],
    "dest": "portal-notificationcenter-demo",
    "externalReference": [],
    "fileMetadata": {},
    "globalMetadata": {
      "breadcrumb_path": "/portal-notificationcenter-demo/breadcrumb/toc.json",
      "extendBreadcrumb": true,
      "feedback_system": "None",
      "author": "kimizhu"
    },
    "markdownEngineName": "markdig",
    "overwrite": [],
    "resource": [
      {
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**"
        ],
        "files": [
          "**/*.png",
          "**/*.jpg"
        ]
      }
    ],
    "template": []
  }
}
